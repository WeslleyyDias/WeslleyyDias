<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".FormSuporteSAC">

    <TextView
        android:id="@+id/suporte"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Suporte [SAC]"
        android:textSize="30sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:id="@+id/suporte1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Como falar com agente?"
        android:textSize="20sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:layout_marginTop="20dp"
        android:layout_marginRight="130dp"
        app:layout_constraintTop_toBottomOf="@id/suporte"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />
    <ImageView
        android:id="@+id/suporte2"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/ic_call"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@id/suporte1"
        app:layout_constraintStart_toStartOf="parent" />
    <TextView
        android:id="@+id/suporte3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Telefones: 4020-8833 ou 0800-221-3388"
        android:textSize="18sp"
        android:textStyle="bold"
        android:layout_marginTop="40dp"
        android:layout_marginLeft="1dp"
        app:layout_constraintTop_toBottomOf="@id/suporte1"
        app:layout_constraintStart_toEndOf="@id/suporte2"/>
    <ImageView
        android:id="@+id/suporte4"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:src="@drawable/wpp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@id/suporte3"
        app:layout_constraintStart_toStartOf="parent" />
    <TextView
        android:id="@+id/suporte5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tambem tems nosso WhatSapp"
        android:textSize="18sp"
        android:textStyle="bold"
        android:layout_marginTop="40dp"
        android:layout_marginLeft="10dp"
        app:layout_constraintTop_toBottomOf="@id/suporte3"
        app:layout_constraintStart_toEndOf="@id/suporte2"/>
    <TextView
        android:id="@+id/suporte6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="(11) 94950-9911"
        android:textSize="20sp"
        android:textStyle="bold"
        android:layout_marginTop="30dp"
        android:layout_marginRight="190dp"
        app:layout_constraintTop_toBottomOf="@id/suporte5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />
    <ImageView
        android:id="@+id/suporte7"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/ic_email"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@id/suporte6"
        app:layout_constraintStart_toStartOf="parent" />
    <TextView
        android:id="@+id/suporte8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Atendimento via E-mail"
        android:textSize="18sp"
        android:textStyle="bold"
        android:layout_marginTop="35dp"
        android:layout_marginLeft="20dp"
        app:layout_constraintTop_toBottomOf="@id/suporte6"
        app:layout_constraintStart_toEndOf="@id/suporte2"/>
    <TextView
        android:id="@+id/suporte9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/suportee"
        android:textSize="20sp"
        android:textStyle="bold"
        android:textColor="@color/purple_700"
        android:layout_marginTop="35dp"
        android:layout_marginLeft="20dp"
        app:layout_constraintTop_toBottomOf="@id/suporte8"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:id="@+id/suporte10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Somente para clientes Top Seguros"
        android:textSize="18sp"
        android:textStyle="bold"
        android:textColor="@color/purple_700"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="20dp"
        app:layout_constraintTop_toBottomOf="@id/suporte9"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:id="@+id/suporte11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="e acionamento de Apolice."
        android:textSize="18sp"
        android:textStyle="bold"
        android:textColor="@color/purple_700"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="20dp"
        app:layout_constraintTop_toBottomOf="@id/suporte10"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:id="@+id/suporte12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="meajuda@topseguros.com.br"
        android:textSize="20sp"
        android:textStyle="bold"
        android:textColor="@color/purple_700"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"
        app:layout_constraintTop_toBottomOf="@id/suporte11"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:id="@+id/suporte13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Para duvidas e atendimentos gerais."
        android:textSize="18sp"
        android:textStyle="bold"
        android:textColor="@color/purple_700"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="20dp"
        app:layout_constraintTop_toBottomOf="@id/suporte12"
        app:layout_constraintStart_toStartOf="parent"/>
    <androidx.appcompat.widget.AppCompatButton

        style="@style/Button"
        android:text="Voltar"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toBottomOf="@id/suporte13"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        />




</androidx.constraintlayout.widget.ConstraintLayout>